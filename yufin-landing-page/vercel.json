{
  "version": 2,
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(ico|png|jpg|jpeg|gif|svg))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index-clean.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
